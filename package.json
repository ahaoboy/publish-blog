{
  "name": "publish-blog",
  "displayName": "publish-blog",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "repository": "https://github.com/ahaoboy/publish-blog.git",
  "publisher": "ahaoboy",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:publish-blog.publish",
    "onLanguage:markdown"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "publish-blog.publish",
        "title": "publishBlog"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "publish-blog.publish",
          "when": "editorLangId == markdown"
        }
      ],
      "editor/context": [
        {
          "when": "editorLangId == markdown",
          "command": "publish-blog.publish",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "editorLangId == markdown",
          "command": "publish-blog.publish",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "",
      "properties": {
        "publish-blog.cookie": {
          "type": "string",
          "default": "cookie",
          "description": "osc cookie"
        },
        "publish-blog.mainPage": {
          "type": "string",
          "default": "https://my.oschina.net/yourName",
          "description": "个人主页"
        }
      }
    }
  },
  "externals": [
    "vscode"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "watch": "parcel   ./src/extension.js -d dist  --target node",
    "build": "parcel build  ./src/extension.js -d dist  --target node"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "js-yaml": "^3.14.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.0",
    "qs": "^6.9.4",
    "yaml": "^1.10.0"
  }
}
